<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="220.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/9.0.1"
            fx:controller="Subapps.Logger.Actions.ImportLogin.ImportLoginController">
    <children>
        <Label alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0"
               text="Import Login Files:" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
               AnchorPane.topAnchor="10.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" text="Files that will be loaded:" AnchorPane.leftAnchor="10.0"
               AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="45.0"/>
        <Button layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" mnemonicParsing="false" onAction="#Chosefile" prefHeight="30.0" prefWidth="130.0"
                text="File Select" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0"/>
        <HBox alignment="CENTER" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0">
            <children>
                <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                        mnemonicParsing="false" onAction="#Save" prefHeight="30.0" prefWidth="130.0" text="Import"
                        HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#Cancel" prefHeight="30.0" prefWidth="130.0" text="Cancel"
                        textAlignment="CENTER"/>
            </children>
        </HBox>
        <ListView fx:id="listview" layoutX="30.0" layoutY="52.0" prefHeight="65.0" prefWidth="200.0"
                  AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="70.0"/>
    </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="Menu" prefHeight="220.0" prefWidth="150.0"
      xmlns="http://javafx.com/javafx/9.0.1" fx:controller="Subapps.Logger.Menu.MenuController">
    <children>
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" onAction="#Addanewlogin" prefHeight="44.0" prefWidth="150.0"
                text="Add  a new login "/>
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" onAction="#ImportLogin" prefHeight="44.0" prefWidth="150.0"
                text="Import logins from file"/>
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" onAction="#ExportLoginlist" prefHeight="44.0" prefWidth="150.0"
                text="Export a list of logins"/>
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" onAction="#About" prefHeight="44.0" prefWidth="150.0" text="About"/>
        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" onAction="#Close" prefHeight="44.0" prefWidth="150.0" text="Close"/>
    </children>
</VBox>
